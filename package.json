    "@react-navigation/native-stack": "6.9.17",
    "@react-navigation/stack": "6.3.20",
    "crypto": "^1.0.1",
    "crypto-js": "^4.2.0",
    "expo": "^52.0.0",
    "expo-av": "~15.0.2",
    "expo-camera": "~16.0.18",
    "expo-font": "~13.0.4",
    "expo-image": "~2.0.7",
    "expo-localization": "~16.0.1",
    "expo-location": "~18.0.10",
    "expo-media-library": "~17.0.6",
